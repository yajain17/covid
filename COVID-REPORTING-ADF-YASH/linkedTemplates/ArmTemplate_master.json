{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "COVID-REPORTING-ADF-YASH"
		},
		"link_casedeath_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'link_casedeath'"
		},
		"ls_ablob_covidbsayash_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_ablob_covidbsayash'"
		},
		"ls_adls_covidreportingdaalake_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_adls_covidreportingdaalake'"
		},
		"link_casedeath_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://github.com"
		},
		"link_casedeath_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "yashjain"
		},
		"ls_adls_covidreportingdaalake_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://covidreportingdaalake.dfs.core.windows.net"
		},
		"trigger_hospital_data_properties_PL_ECBA_covid_parameters_sourcerealtiveurl": {
			"type": "string",
			"defaultValue": "cloudboxacademy/covid19/raw/main/ecdc_data/hospital_admissions.csv"
		},
		"trigger_hospital_data_properties_PL_ECBA_covid_parameters_sinkfilename": {
			"type": "string",
			"defaultValue": "hospitaL_trigger.csv"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "COVID-REPORTING-ADF-YASH_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"link_casedeath_password": {
						"value": "[parameters('link_casedeath_password')]"
					},
					"ls_ablob_covidbsayash_connectionString": {
						"value": "[parameters('ls_ablob_covidbsayash_connectionString')]"
					},
					"ls_adls_covidreportingdaalake_accountKey": {
						"value": "[parameters('ls_adls_covidreportingdaalake_accountKey')]"
					},
					"link_casedeath_properties_typeProperties_url": {
						"value": "[parameters('link_casedeath_properties_typeProperties_url')]"
					},
					"link_casedeath_properties_typeProperties_userName": {
						"value": "[parameters('link_casedeath_properties_typeProperties_userName')]"
					},
					"ls_adls_covidreportingdaalake_properties_typeProperties_url": {
						"value": "[parameters('ls_adls_covidreportingdaalake_properties_typeProperties_url')]"
					},
					"trigger_hospital_data_properties_PL_ECBA_covid_parameters_sourcerealtiveurl": {
						"value": "[parameters('trigger_hospital_data_properties_PL_ECBA_covid_parameters_sourcerealtiveurl')]"
					},
					"trigger_hospital_data_properties_PL_ECBA_covid_parameters_sinkfilename": {
						"value": "[parameters('trigger_hospital_data_properties_PL_ECBA_covid_parameters_sinkfilename')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}